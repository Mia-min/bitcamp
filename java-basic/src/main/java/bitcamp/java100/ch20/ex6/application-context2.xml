<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="CarFactory" class="bitcamp.java100.ch28.ex6.CarFactory2">
        <property name="tires">
            <array>
                <baen class="bitcamp.java100.ch28.ex6.Tire" p:marker="한국타이어"/>
                <baen class="bitcamp.java100.ch28.ex6.Tire" p:marker="한국타이어"/>
                <baen class="bitcamp.java100.ch28.ex6.Tire" p:marker="금호타이어"/>
                <baen class="bitcamp.java100.ch28.ex6.Tire" p:marker="금호타이어"/>
            </array>
        </property>
    </bean>

    <bean id="c1" factory-bean="CarFactory"
          factory-method="createCar">

          <constructor-arg value="티코 골드"/>
    </bean>
    
    <bean id="c2" factory-bean="CarFactory"
          factory-method="createCar">
          <constructor-arg value="에쿠스"/>
    </bean>
    
    <bean id="c3" factory-bean="CarFactory"
          factory-method="createCar">
          <constructor-arg value="소나타"/>
    </bean>
    
</beans>















